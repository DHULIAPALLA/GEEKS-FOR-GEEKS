class Solution:
    #Function to find triplets with zero sum.    
    def findTriplets(self, arr, n):
        for i in range(n-2):
            for j in range(i+1,n-1):
                for k in range(j+1,n):
                    if arr[i]+arr[j]+arr[k]==0:
                        return 0
                    else:
                        return 1
        
                
